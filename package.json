{
	"name": "@pixelpizza/ourtube",
	"version": "1.0.0",
	"description": "A music discord bot written in the sapphire framework",
	"main": "dist/sharder.js",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"update": "yarn up \"*\" -i",
		"build": "tsup && tsc --p tsconfig.lang.json && tsc --noEmit",
		"lint": "eslint src --ext mjs,ts --fix",
		"format": "prettier --write **/*.{ts,js,json,yml,yaml}",
		"prepack": "pinst --disable",
		"prepare": "husky install",
		"postpack": "pinst --enable",
		"start": "yarn build && node ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PixelPizza/OurTube.git"
	},
	"keywords": [],
	"author": "@pixelpizza",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/PixelPizza/OurTube/issues"
	},
	"homepage": "https://github.com/PixelPizza/OurTube#readme",
	"dependencies": {
		"@discord-player/extractor": "^4.4.4",
		"@discordjs/opus": "^0.9.0",
		"@kaname-png/plugin-statcord": "^2.2.0",
		"@prisma/client": "^5.4.2",
		"@sapphire/decorators": "^6.0.2",
		"@sapphire/framework": "^4.6.1",
		"@sapphire/plugin-i18next": "^5.1.1",
		"@sapphire/plugin-logger": "^3.0.6",
		"@sapphire/shapeshift": "^3.9.2",
		"@sapphire/time-utilities": "^1.7.10",
		"common-tags": "^1.8.2",
		"discord-player": "^6.6.4",
		"discord.js": "^14.16.1",
		"dotenv": "^16.3.1",
		"sodium-native": "^4.0.4",
		"soundcloud-scraper": "^5.0.3",
		"spotify-url-info": "^3.2.8",
		"tslib": "^2.6.2",
		"youtube-sr": "^4.3.4",
		"ytdl-core": "^4.11.5"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.8.0",
		"@sapphire/eslint-config": "5.0.2",
		"@sapphire/eslint-plugin-result": "^2.0.1",
		"@sapphire/ts-config": "4.0.1",
		"@types/common-tags": "1.8.3",
		"@types/eslint": "8.44.6",
		"@types/node": "18.18.6",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.8.0",
		"@typescript-eslint/parser": "6.8.0",
		"eslint": "8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "alpha",
		"husky": "8.0.3",
		"pinst": "^3.0.0",
		"prettier": "3.0.3",
		"prisma": "5.4.2",
		"tsup": "7.2.0",
		"typescript": "5.2.2"
	},
	"packageManager": "yarn@3.6.4"
}
