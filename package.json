{
	"name": "ourtube",
	"version": "1.0.0",
	"description": "A music discord bot written in discord.js",
	"main": "dist/sharder.js",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"update": "yarn up \"*\" -i",
		"clean": "node scripts/clean-dist.mjs",
		"build": "yarn clean && tsup && tsc --p tsconfig.lang.json",
		"lint": "eslint src scripts --ext mjs,ts --fix",
		"format": "prettier --write **/*.{ts,js,json,yml,yaml}",
		"prepare": "husky install",
		"start": "yarn build && node ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PixelPizza/OurTube.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/PixelPizza/OurTube/issues"
	},
	"homepage": "https://github.com/PixelPizza/OurTube#readme",
	"dependencies": {
		"@discordjs/builders": "^0.13.0",
		"@discordjs/opus": "^0.7.0",
		"@kaname-png/plugin-statcord": "^1.3.4",
		"@prisma/client": "^3.14.0",
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/framework": "^3.0.0-next.fd7be0b.0",
		"@sapphire/pieces": "^3.3.3",
		"@sapphire/plugin-i18next": "^2.5.1",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/time-utilities": "^1.7.4",
		"common-tags": "^1.8.2",
		"discord-player": "^5.2.2",
		"discord.js": "^13.7.0",
		"dotenv": "^16.0.1",
		"sodium": "^3.0.2",
		"tslib": "^2.4.0",
		"ytdl-core": "^4.11.0"
	},
	"devDependencies": {
		"@commitlint/config-angular": "17.0.0",
		"@sapphire/eslint-config": "4.3.5",
		"@sapphire/ts-config": "3.3.4",
		"@types/common-tags": "1.8.1",
		"@types/eslint": "8.4.2",
		"@types/node": "17.0.21",
		"@types/prettier": "2.6.1",
		"@typescript-eslint/eslint-plugin": "5.25.0",
		"@typescript-eslint/parser": "5.25.0",
		"eslint": "8.15.0",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "8.0.1",
		"prettier": "2.6.2",
		"prisma": "3.14.0",
		"tsup": "5.12.8",
		"typescript": "4.6.4"
	}
}
