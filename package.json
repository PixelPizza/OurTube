{
  "name": "ourtube",
  "version": "1.0.0",
  "description": "A music discord bot written in discord.js",
  "main": "dist/sharder.mjs",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "update": "yarn up \"*\" -i",
    "clean": "node scripts/clean-dist.mjs",
    "build": "yarn clean && tsup && tsc --p tsconfig.lang.json",
    "lint": "eslint --ext mjs,js,ts",
    "lint:fix": "eslint --ext mjs,js,ts --fix",
    "format": "prettier --write **/*.{ts,js,json,yml,yaml}",
    "start": "yarn build && node dist/sharder.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JTTechnic/OurTube.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JTTechnic/OurTube/issues"
  },
  "homepage": "https://github.com/JTTechnic/OurTube#readme",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/opus": "^0.5.3",
    "@sapphire/decorators": "^4.2.3",
    "@sapphire/framework": "^3.0.0-next.9ed3d98.0",
    "@sapphire/pieces": "^3.2.0",
    "@sapphire/plugin-i18next": "^2.2.3",
    "@sapphire/plugin-logger": "^2.1.3",
    "@sapphire/time-utilities": "^1.6.0",
    "common-tags": "^1.8.2",
    "discord-player": "^5.2.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "sodium": "^3.0.2",
    "tslib": "^2.3.1",
    "ytdl-core": "^4.10.1"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.3",
    "@types/common-tags": "^1.8.1",
    "@types/node": "^17.0.17",
    "prettier": "^2.5.1",
    "tsup": "^5.12.1",
    "typescript": "^4.5.5"
  }
}
