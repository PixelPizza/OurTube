{
	"name": "ourtube",
	"version": "1.0.0",
	"description": "A music discord bot written in discord.js",
	"main": "dist/sharder.js",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"update": "yarn up \"*\" -i",
		"clean": "node scripts/clean-dist.mjs",
		"build": "yarn clean && tsup && tsc --p tsconfig.lang.json && tsc --noEmit",
		"lint": "eslint src scripts --ext mjs,ts --fix",
		"format": "prettier --write **/*.{ts,js,json,yml,yaml}",
		"prepare": "husky install",
		"start": "yarn build && node ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PixelPizza/OurTube.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/PixelPizza/OurTube/issues"
	},
	"homepage": "https://github.com/PixelPizza/OurTube#readme",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@kaname-png/plugin-statcord": "^2.1.10",
		"@prisma/client": "^4.14.1",
		"@sapphire/decorators": "^6.0.1",
		"@sapphire/framework": "pr-512",
		"@sapphire/plugin-i18next": "^5.0.3",
		"@sapphire/plugin-logger": "^3.0.4",
		"@sapphire/time-utilities": "^1.7.9",
		"common-tags": "^1.8.2",
		"discord-player": "^5.4.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.0.3",
		"sodium-native": "^4.0.1",
		"soundcloud-scraper": "^5.0.3",
		"spotify-url-info": "^3.2.5",
		"tslib": "^2.5.1",
		"youtube-sr": "^4.3.4",
		"ytdl-core": "^4.11.4"
	},
	"devDependencies": {
		"@commitlint/config-angular": "17.6.3",
		"@sapphire/eslint-config": "4.4.2",
		"@sapphire/eslint-plugin-result": "^1.0.2",
		"@sapphire/ts-config": "3.3.4",
		"@types/common-tags": "1.8.1",
		"@types/eslint": "8.37.0",
		"@types/node": "18.16.13",
		"@types/prettier": "2.7.2",
		"@typescript-eslint/eslint-plugin": "5.59.6",
		"@typescript-eslint/parser": "5.59.6",
		"eslint": "8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "8.0.3",
		"prettier": "2.8.8",
		"prisma": "4.14.1",
		"tsup": "6.7.0",
		"typescript": "4.9.5"
	},
	"packageManager": "yarn@3.5.1"
}
